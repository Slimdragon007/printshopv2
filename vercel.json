{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)\\.(css|js|png|jpg|jpeg|gif|svg|webp|ico)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/archive", "destination": "/404.html" },
    { "source": "/archive/(.*)", "destination": "/404.html" }
  ]
}
